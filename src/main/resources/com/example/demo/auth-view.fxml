<AnchorPane prefHeight="549.0" prefWidth="774.0" style="-fx-background-color: #EDEDED;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.controleur.AuthController">

   <children>

      <Label fx:id="codeerror" layoutX="266.0" layoutY="127.0" prefHeight="36.0" prefWidth="443.0" textFill="#ff0606">
         <font>
            <Font name="System Bold Italic" size="21.0" />
         </font>
      </Label>
      <Label fx:id="mail" layoutX="106.0" layoutY="189.0" prefHeight="36.0" prefWidth="145.0" text="Email">
         <font>
            <Font name="System Bold Italic" size="21.0" />
         </font>
      </Label>
      <Label layoutX="106.0" layoutY="275.0" prefHeight="36.0" prefWidth="160.0" text="Mot de passe">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Label>
      <Button fx:id="submitButton" layoutX="417.0" layoutY="383.0" mnemonicParsing="false" onAction="#login" text="Connexion">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
      <TextField fx:id="emailIdField" layoutX="268.0" layoutY="189.0" prefHeight="36.0" prefWidth="443.0" onMouseClicked="#clean" >
         <font>
            <Font size="19.0" />
         </font></TextField>
      <PasswordField fx:id="passwordField" layoutX="268.0" layoutY="275.0" prefHeight="36.0" prefWidth="443.0" onMouseClicked="#clean">
